	<!-- Flap lever -->
	<animation>
		<name>Flap lever pick</name>
		<type>pick</type>
		<object-name>flapselector</object-name>
		<action>
			<button>0</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>nasal</command>
				<script>
				 controls.flapsDown(1);
				</script>
			</binding>
		</action>
		<action>
			<button>1</button>
			<repeatable type="bool">false</repeatable>
			<binding>
				<command>nasal</command>
				<script>
				 controls.flapsDown(-1);
				</script>
			</binding>
		</action>
	</animation>
